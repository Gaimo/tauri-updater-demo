  "productName": "tauri-updater-demo",
  "version": "0.1.1",
  "identifier": "com.admin.tauri-updater-demo",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-updater-demo",
